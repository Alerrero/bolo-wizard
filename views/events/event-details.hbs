<h4>Detalles evento</h4>
<ul>

    <li>{{event.name}}</li>
    <li>{{event.dates.start.localDate}}</li>
    <li>{{event._embedded.venues.[0].name}}</li>
    <li><img src="{{event.images.[0].url}}" alt=""></li>

</ul>

{{#each tracks}}
{{#if preview_url}}
<h4>{{name}}</h4>
<audio src="{{preview_url}}" controls></audio>
{{/if}}
{{/each}}

<a href="{{artist.external_urls.spotify}}" target="_blank">Link</a>

<figure id="event-map"></figure>
<p>{{place.formatted_address}}</p>

<script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
<script src="/javascripts/map-config.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key={{mapsKey}}&callback=initMap"></script>